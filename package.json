    "@popperjs/core": "^2.11.0",
    "babel-loader": "^8.2.3",
    "bootstrap": "^5.1",
    "clean-webpack-plugin": "^4.0.0",
    "css-loader": "^6.7.1",
    "html-loader": "^3.1",
    "html-webpack-plugin": "^5.5",
    "jquery": "^3.6",
