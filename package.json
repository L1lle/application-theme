{
  "name": "bootstrap-simple-application-theme",
  "version": "4.2.0",
  "description": "A lightweight application theme build on top of Bootstrap4",
  "main": "dist/app-theme.js",
  "scripts": {
    "build": "npm run webpack && npm run scss",
    "scss": "node-sass --importer node_modules/node-sass-package-importer/dist/cli.js src/sass/app.scss -o dist",
    "webpack": "webpack"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/L1lle/application-theme.git"
  },
  "keywords": [
    "theme",
    "bootstrap",
    "bootstrap4"
  ],
  "author": "Matthias Lill",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/L1lle/application-theme/issues"
  },
  "homepage": "https://github.com/L1lle/application-theme#readme",
  "devDependencies": {
    "@babel/core": "^7.2.2",
    "@babel/preset-env": "^7.3.1",
    "babel-loader": "^8.0.5",
    "bootstrap": "4.2.1",
    "clean-webpack-plugin": "^1.0.1",
    "html-loader": "^0.5.5",
    "html-webpack-plugin": "^3.2.0",
    "jquery": "^3.3.1",
    "node-sass": "^4.9.0",
    "node-sass-package-importer": "^5.3.0",
    "popper.js": "^1.14.0",
    "webpack": "^4.29.0",
    "webpack-cli": "^3.2.1"
  }
